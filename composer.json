{
  "name": "darrynten/watson-personality-insights-php",
  "description": "PHP Watson Personality Insights Client with Extras",
  "type": "library",
  "require": {
    "php": "^5.5.9 || ^7.0",
    "guzzlehttp/guzzle": "^6.2.1",
    "darrynten/any-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "dev-master"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "DarrynTen\\PersonalityInsightsPhp\\": "src/"
    },
    "exclude-from-classmap": ["/Tests/"]
  }
}
